<%= render 'tickets/notice', notice: notice %>

<table class="table table-striped">
  <thead class="thead-dark">
  <tr>
    <th>Title</th>
    <th>Number</th>
    <th>Due Date</th>
  </tr>
  </thead>

  <tbody>
    <tr>
      <td><%= @ticket.title.humanize %></td>
      <td><%= @ticket.number %></td>
      <td><%= @ticket.due_date %></td>
    </tr>
  </tbody>
</table>
<%= link_to 'Edit', edit_ticket_path(@ticket) %> |
<%= link_to 'Back', tickets_path %>
